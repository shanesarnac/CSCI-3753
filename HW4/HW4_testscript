echo IO Bound Test with "$SIMULATIONS_LOW" Simulations and Non-Uniform Priorities
/usr/bin/time -f "$TIMEFORMAT3" sudo ./test SCHED_RR "$SIMULATIONS_LOW" "$IO_INTENSIVE" "$RAND"

