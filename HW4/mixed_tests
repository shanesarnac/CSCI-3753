echo Mixed Test with "$SIMULATIONS_LOW" Simulations and Uniform Priorities
/usr/bin/time -f "$TIMEFORMAT3" sudo ./test SCHED_RR "$SIMULATIONS_LOW" "$MIXED_INTENSIVE" 
